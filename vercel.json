{
    "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/api/express/(.*)",
      "destination": "/api/express"
    },
    { "src": "/(.*)", "dest": "/src/pages/index.astro" }
  ]
  }
  